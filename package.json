{
  "name": "openfisca-doc",
  "version": "0.5.0",
  "description": "OpenFisca documentation",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfisca/openfisca-doc.git"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "gitbook build",
    "clean": "del _book",
    "deploy": "npm run build && sh deploy.sh",
    "deploy:ci": "sh deploy.sh",
    "postinstall": "gitbook install",
    "start": "gitbook serve --port 2050",
    "test": "sh run-tests.sh"
  },
  "author": "Christophe Benz <christophe.benz@data.gouv.fr>",
  "license": "AGPL-3.0",
  "private": true,
  "devDependencies": {
    "del-cli": "^0.2.1",
    "gh-pages": "^0.12.0",
    "gitbook-cli": "^2.3.0"
  }
}
